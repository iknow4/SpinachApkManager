<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <style name="activity_divider">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/big_divider</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="listview">
        <item name="android:listSelector">@drawable/selector_list_item</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@drawable/divider</item>
        <item name="android:dividerHeight">2.0px</item>
    </style>
    <style name="btn_green">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_btn_green</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:layout_margin">5.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.5</item>
    </style>
    <style name="title_style">
        <item name="android:textSize">17.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_marginLeft">15.0dip</item>
        <item name="android:layout_marginTop">5.0dip</item>
    </style>
    <style name="des_style">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">#ff0065ce</item>
        <item name="android:layout_marginLeft">15.0dip</item>
        <item name="android:layout_marginTop">1.0dip</item>
    </style>
